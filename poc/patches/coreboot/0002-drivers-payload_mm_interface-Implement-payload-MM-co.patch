diff --git a/src/include/payload_mm_interface.h b/src/include/payload_mm_interface.h
new file mode 100644
index 0000000000..361042d3ef
--- /dev/null
+++ b/src/include/payload_mm_interface.h
@@ -0,0 +1,71 @@
+/* SPDX-License-Identifier: GPL-2.0-only */
+
+#ifndef PAYLOAD_MM_INTERFACE_H
+#define PAYLOAD_MM_INTERFACE_H
+
+#include <commonlib/coreboot_tables.h>
+#include <types.h>
+
+/*
+ * The data below is communicated between bootloader and payload.
+ * It describes the data required by the bootloader to call payload MM.
+ */
+#define PLD_MM_SHARED_STRUCT_MAGIC 	0x5f4d4d5f444c505f  /* '_PLD_MM_' */
+#define PLD_MM_SHARED_STRUCT_MAX_SIZE 	4096
+#define PLD_MM_SHARED_STRUCT_REVISION	1
+
+struct PLD_MM_TO_BL_SMM_INFO {
+	uint64_t  header_magic;
+	uint16_t  header_size;
+	uint8_t   header_revision;
+	uint8_t   reserved;
+	uint32_t  mm_entrypoint_address;
+};
+
+void payload_mm_get_reserved_region(uintptr_t *tseg_base, size_t *tseg_size);
+
+void lb_payload_mm(struct lb_header *header);
+
+/* Payload MM command interface */
+
+#define PAYLOAD_MM_RET_SUCCESS 0
+#define PAYLOAD_MM_RET_FAILURE 1
+
+#define PAYLOAD_MM_UNLOCK_SMRAM   1
+#define PAYLOAD_MM_REGISTER_ENTRY 2
+#define PAYLOAD_MM_LOCK_SMRAM     3
+
+/*
+ * Processor information and functionality needed by MM Foundation.
+ */
+struct EFI_MM_ENTRY_CONTEXT {
+  uint64_t  ReservedMmStartupThisAp;
+  /*
+   * A number between zero and the NumberOfCpus field. This field designates which
+   * processor is executing the MM Foundation.
+   */
+  uint64_t  CurrentlyExecutingCpu;
+  /*
+   * The number of possible processors in the platform.  This is a 1 based
+   * counter.  This does not indicate the number of processors that entered MM.
+   */
+  uint64_t  NumberOfCpus;
+  /*
+   * Points to an array, where each element describes the number of bytes in the
+   * corresponding save state specified by CpuSaveState. There are always
+   * NumberOfCpus entries in the array.
+   */
+  uint64_t  CpuSaveStateSize;
+  /*
+   * Points to an array, where each element is a pointer to a CPU save state. The
+   * corresponding element in CpuSaveStateSize specifies the number of bytes in the
+   * save state area. There are always NumberOfCpus entries in the array.
+   */
+  uint64_t  CpuSaveState;
+};
+
+bool payload_mm_installed(void);
+uint8_t payload_mm_exec_interface(uint8_t sub_command, uint32_t argument);
+void payload_mm_call_entrypoint(void);
+
+#endif
