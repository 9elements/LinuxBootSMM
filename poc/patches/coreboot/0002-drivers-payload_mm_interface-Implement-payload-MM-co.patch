+};
+
+struct lb_payload_mm_smram_region {
+	uint32_t tag;
+	uint32_t size;
+	uint32_t number_of_smm_regions;
+	struct lb_pld_smram_descriptor descriptor[1];
+};
+
+/*
+ * Payload MM shared memory address
+ */
+struct lb_payload_mm_shared_mem {
+	uint32_t tag;
